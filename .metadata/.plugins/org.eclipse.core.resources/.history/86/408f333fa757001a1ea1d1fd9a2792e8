		System.out.print("영어 입력: ");
		eng = scan.nextInt();
		System.out.print("수학 입력: ");		
		math = scan.nextInt();
	}
	@Override
	public void calc() {
		
	}

	@Override
	public void display() {
		// TODO Auto-generated method stub
		
	}

}
