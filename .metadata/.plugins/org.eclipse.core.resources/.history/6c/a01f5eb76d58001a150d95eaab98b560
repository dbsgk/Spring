	@Override
	public void display() {
		System.out.println("이름\t국어\t수학\t영어\t총점\t평균\t");
		System.out.println(sungjukDTO.getName()+"\t"+sungjukDTO.getKor()+"\t"+sungjukDTO.getEng()+"\t"+sungjukDTO.getMath()+"\t"+sungjukDTO.getTotal()+"\t"+String.format("%.3f", sungjukDTO.getAvg()));
	}

